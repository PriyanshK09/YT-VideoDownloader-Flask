# YouTube Downloader Configuration

# Enable PO Token for bot detection bypass (recommended: true)
# This helps avoid "This request was detected as a bot" errors
# See: https://pytubefix.readthedocs.io/en/latest/user/po_token.html
YOUTUBE_USE_PO_TOKEN=true

# Enable OAuth authentication (optional: false)
# Set to true for additional authentication if needed
YOUTUBE_USE_OAUTH=false
